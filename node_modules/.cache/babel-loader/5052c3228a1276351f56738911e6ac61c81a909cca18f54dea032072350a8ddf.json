n=\"round\"\n                        />\n                        <path\n                            d=\"M10 4L10 20\"\n                            stroke=\"#171625\"\n                            stroke-width=\"1.5\"\n                            stroke-linecap=\"round\"\n                            stroke-linejoin=\"round\"\n                            stroke-dasharray=\"5 5\"\n                        />\n                    </svg>\n                    <div class=\"title\">Expired Tickets</div>\n                </div>\n                <div class=\"bottom\">{{ user.eventsInactive }}</div>\n            </div>\n        </div>\n        <div class=\"bottom\">\n            <div class=\"title\">All Tickets</div>\n            <div class=\"table\">\n                <table class=\"table-container\">\n                    <tr class=\"columns\">\n                        <th>Image</th>\n                        <th>Event Name</th>\n                        <th>Contact Name</th>\n                        <th>Phone Number</th>\n                        <th>Max Tickets</th>\n                        <th>Description</th>\n                        <th>Location</th>\n                        <th>Status</th>\n                    </tr>\n                    <tr v-for=\"(ticket, index) in user.allTickets\">\n                        <td v-if=\"index >= pageNo && index < pageNo + 5\">\n                            <img\n                                :src=\"`${ticket.image}`\"\n                                alt=\"\"\n                            />\n                        </td>\n                        <td v-if=\"index >= pageNo && index < pageNo + 5\">\n                            {{ ticket.eventName }}\n                        </td>\n                        <td v-if=\"index >= pageNo && index < pageNo + 5\">\n                            {{ ticket.contactName }}\n                        </td>\n                        <td v-if=\"index >= pageNo && index < pageNo + 5\">\n                           