it: {\n              eventName:'',\n            contactName:'',\n            phoneNumber:'',\n            maxTickets:'',\n            description: '',\n            location: ''\n            }\n            \n\n        };\n    },\n    computed: {\n        totalNumberOfPages() {\n            return Math.floor(this.user.allTickets.length / 5);\n        },\n        notFilled () {\n          return !this.edit.eventName || !this.edit.contactName || !this.edit.phoneNumber || !this.edit.maxTickets || !this.edit.description || !this.edit.location;\n        }\n\n    },\n    methods: {\n        clickOptions(no) {\n            if (this.clickedOptions == no) {\n                this.clickedOptions = 6;\n            } else {\n                this.clickedOptions = no;\n            }\n        },\n        changePageNo(no) {\n            this.pageNo = no;\n        },\n        decrease() {\n            if (this.pageNo > 0) {\n                this.pageNo -= 1;\n            }\n        },\n        increase() {\n            if (this.pageNo < this.totalNumberOfPages) {\n                this.pageNo += 1;\n            }\n        },\n    },\n};\n</script>\n"],"mappings":"AAiaA,SAASA,QAAO,QAAS,KAAK;AAE9B,eAAe;EACXC,KAAK,EAAE,CAAC,MAAM,EAAE,UAAU,CAAC;EAC3BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,cAAc,EAAEC,GAAG;MACnBC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE,CAAC;MACTC,IAAI,EAAE;QACJC,SAAS,EAAC,EAAE;QACdC,WAAW,EAAC,EAAE;QACdC,WAAW,EAAC,EAAE;QACdC,UAAU,EAAC,EAAE;QACbC,WAAW,EAAE,EAAE;QACfC,QAAQ,EAAE;MACV;IAGJ,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,kBAAkBA,CAAA,EAAG;MACjB,OAAOC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACC,IAAI,CAACC,UAAU,CAACC,MAAK,GAAI,CAAC,CAAC;IACtD,CAAC;IACDC,SAAQA,CAAA,EAAK;MACX,OAAO,CAAC,IAAI,CAACd,IAAI,CAACC,SAAQ,IAAK,CAAC,IAAI,CAACD,IAAI,CAACE,WAAU,IAAK,CAAC,IAAI,CAACF,IAAI,CAACG,WAAU,IAAK,CAAC,IAAI,CAACH,IAAI,CAACI,UAAS,IAAK,CAAC,IAAI,CAACJ,IAAI,CAACK,WAAU,IAAK,CAAC,IAAI,CAACL,IAAI,CAACM,QAAQ;IAC3J;EAEJ,CAAC;EACDS,OAAO,EAAE;IACLC,YAAYA,CAACC,EAAE,EAAE;MACb,IAAI,IAAI,CAACrB,cAAa,IAAKqB,EAAE,EAAE;QAC3B,IAAI